{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": true,
    "jsx": "react",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "webworker",
      "dom",
      "scripthost"]
  },
  "exclude": [
    "node_modules"
  ],
  "baseUrl": "types",
  "typeRoots": ["types"]
}